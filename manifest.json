{
  "manifest_version": 3,
  "name": "CommentWizard",
  "description": "Generates suggestions using ChatGPT for LinkedIn comments.",
  "version": "0.0.1",
  "content_scripts": [
    {
      "matches": ["https://www.linkedin.com/*"],
      "js": ["content.js"]
    }
  ],
  "permissions": ["storage"],
  "action": {
    "default_icon": "icon.png"
  }
}
